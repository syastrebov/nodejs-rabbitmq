{
    nonew    : true,
    curly    : true,
    unused   : strict,
    noarg    : true,
    indent   : 4,
    forin    : false,
    noempty  : true,
    eqeqeq   : false,
    strict   : true,
    undef    : true,
    bitwise  : true,
    immed    : true,
    browser  : true,
    camelcase: false,
    devel    : true,
    jquery   : true,
    globals  : {
        describe     : true,
        beforeEach   : true,
        afterEach    : true,
        it           : true,
        expect       : true,
        inject       : true,
        require      : true,
        exports      : true,
        setImmediate : true,
        waitsFor     : true
    }
}

